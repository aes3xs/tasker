        "symfony/config": "~2.7|~3.0",
        "symfony/console": "~2.7|~3.0",
        "symfony/expression-language": "~2.7|~3.0",
        "symfony/filesystem": "~2.7|~3.0",
        "symfony/process": "~2.7|~3.0",
        "twig/twig": "~1.32",
        "doctrine/inflector": "^1.1",
        "phpseclib/phpseclib": "^2.0",
        "herzult/php-ssh": "~1.0",
        "monolog/monolog": "~1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.2"
    },
    "autoload": {
